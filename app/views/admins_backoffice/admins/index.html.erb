<br>
<h1>Lista Administradores</h1>
<hr/>
<br>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th scope="col">
        <%= link_to new_admins_backoffice_admin_path	, class:"btn btn-success" do %>
          +
        <% end %>
      </th>
    </tr>
  </thead>
  <tbody>
  <% @admins.each do |admin| %>
    <tr>
      <th scope="row"><%=admin.id%></th>
      <td><%=admin.email%></td>
      <td>
      <%= link_to edit_admins_backoffice_admin_path(admin), class:"btn btn-warning" do %>
        Editar
      <% end %>
      </td>
      <td><button type="button" class="btn btn-danger">Apagar</button></td>
    </tr>
  <% end %>
  </tbody>
</table>